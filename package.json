{
  "name": "synapse-digest-bot",
  "version": "0.1.0",
  "description": "Community Digest AI Bot for Discord → LLM → Slack",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --coverage.provider=v8 --coverage.reporter=text-summary --coverage.reporter=json-summary --coverage.reporter=lcov",
    "test:watch": "vitest",
    "lint": "echo \"TODO: add eslint\" && exit 0",
    "discord:debug": "ts-node-dev --transpile-only src/tools/discord_debug.ts",
    "models:list": "ts-node-dev --transpile-only src/tools/gemini_list_models.ts",
    "discourse:debug": "ts-node-dev --transpile-only src/tools/discourse_debug.ts"
  },
  "keywords": [
    "discord",
    "slack",
    "digest",
    "llm",
    "community"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=24 <25"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.29",
    "@google/generative-ai": "^0.24.1",
    "@slack/web-api": "^7.14.1",
    "ai": "^6.0.89",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "node-fetch": "^3.3.2",
    "p-retry": "^7.1.1",
    "pino": "^10.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/node-fetch": "^2.6.13",
    "@vitest/coverage-v8": "^4.0.18",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "diff": "^4.0.4",
    "undici": "^6.23.0"
  }
}
